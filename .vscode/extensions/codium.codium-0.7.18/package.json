{
	"name": "codium",
	"publisher": "Codium",
	"displayName": "CodiumAI - Integrity Agent powered by GPT-3.5&4",
	"icon": "media/logo/icon.png",
	"description": "Your code integrity AI agent - analyzes your code and generates meaningful tests to catch bugs before you ship (powered by GPT-3.5&4 & TestGPT-1)",
	"galleryBanner": {
		"color": "#684BFE",
		"theme": "dark"
	},
	"version": "0.7.18",
	"repository": {
		"type": "git",
		"url": "https://github.com/Codium-ai/codiumai-vscode-release"
	},
	"homepage": "https://www.codium.ai/",
	"keywords": [
		"copilot",
		"codium",
		"openai",
		"chatgpt",
		"gpt-3",
		"python",
		"code-inegrity",
		"unit-testing",
		"code-coverage",
		"generative-ai"
	],
	"license": "MIT",
	"engines": {
		"vscode": "^1.72.0"
	},
	"categories": [
		"Testing",
		"Machine Learning",
		"Data Science",
		"Snippets",
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:python",
		"onStartupFinished",
		"onUri",
		"onView:open-sidebar"
	],
	"extensionDependencies": [
		"vscode.git"
	],
	"author": {
		"name": "Codium"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "codium-sidebar-view-container",
					"title": "CodiumAI",
					"icon": "media/logo/codium-logo-white.svg"
				}
			]
		},
		"views": {
			"codium-sidebar-view-container": [
				{
					"type": "webview",
					"id": "chat-view",
					"name": "Chat",
					"size": 2,
					"visibility": "visible"
				}
			]
		},
		"commands": [
			{
				"command": "codium.enable",
				"shortTitle": "Enable CodiumAI",
				"title": "Enable CodiumAI"
			},
			{
				"command": "codium.auth.accountSignIn",
				"title": "Sign in to CodiumAI"
			},
			{
				"command": "codium.disable",
				"title": "Disable CodiumAI",
				"shortTitle": "Disable CodiumAI"
			},
			{
				"command": "codium.getStarted",
				"title": "CodiumAI - Get Started",
				"category": "CodiumAI",
				"icon": "$(question)"
			},
			{
				"command": "codium.generate_tests_from_selection",
				"title": "Generate Tests",
				"category": "CodiumAI",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.generate_tests_from_shortcut",
				"title": "Generate Tests",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.right_click_explain_code",
				"title": "Explain this code",
				"category": "CodiumAI",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.right_click_write_docstring",
				"title": "Write Docstring for this code",
				"category": "CodiumAI",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.right_click_enhance_code",
				"title": "Enhance this code",
				"category": "CodiumAI",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.right_click_code_suggestion",
				"title": "Get code suggestions",
				"category": "CodiumAI",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.clearCaches",
				"title": "CodiumAI - Clear Caches",
				"enablement": "codium.user.isLoggedIn"
			},
			{
				"command": "codium.chat.clear",
				"title": "Clear Chat",
				"category": "CodiumAI",
				"icon": "$(clear-all)"
			},
			{
				"command": "codium.chat.reload",
				"title": "Reset Chat",
				"category": "CodiumAI",
				"icon": "$(refresh)"
			}
		],
		"keybindings": [
			{
				"command": "codium.generate_tests_from_shortcut",
				"mac": "cmd+ctrl+c",
				"key": "alt+shift+c",
				"windows": "ctrl+win+z",
				"when": "true"
			}
		],
		"configuration": {
			"title": "CodiumAI",
			"properties": {
				"codium.enable": {
					"type": "boolean",
					"default": true,
					"description": "Enable CodiumAI plugin"
				},
				"codium.editorButton": {
					"type": "boolean",
					"default": true,
					"description": "Show CodiumAI buttons above functions and classes in the code editors (**Note**: You can still run codium from the sidebar)"
				},
				"codium.javascriptTestsConfig-1RunWorkingDirectory": {
					"type": "string",
					"default": "",
					"description": "When running Javascript / Typescript tests, use this directory as the working directory.\n- This is typically the ROOT DIRECTORY OF YOU PROJECT, where your package.json lives.\n- Use ABSOLUTE PATH.\n\nPLEASE CONFIGURE BOTH THIS AND RUN-COMMAND."
				},
				"codium.javascriptTestsConfig-2RunCommand": {
					"type": "string",
					"default": "",
					"description": "Use this command for running a single Javascript / Typescript tests file.\n- The placeholder TEST_FILEPATH will be replaced with the actual test file path.\n- You almost always want to start with 'npx' (e.g. 'npx jest', not 'npm jest' or 'yarn test')\n- Note that the test command must be able to run test files that are located in the same directory as the file under test.\n\nExamples:\n - npx jest --runTestsByPath TEST_FILEPATH\n - npx ts-mocha TEST_FILEPATH --require ./test/mocha/setup.ts\n\nPLEASE CONFIGURE BOTH THIS AND WORKING-DIRECTORY."
				},
				"codium.javascriptTestsConfig-3RunDefaultImports": {
					"type": "string",
					"default": "",
					"description": "When running javascript test files, always add these imports at the beginning of the file. Separate with '\\n'."
				}
			}
		},
		"submenus": [
			{
				"id": "codium.submenu",
				"label": "CodiumAI"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "codium.submenu",
					"group": "codium@1"
				}
			],
			"codium.submenu": [
				{
					"command": "codium.generate_tests_from_selection",
					"group": "a@0"
				},
				{
					"command": "codium.right_click_explain_code",
					"group": "b@1"
				},
				{
					"command": "codium.right_click_write_docstring",
					"group": "b@2"
				},
				{
					"command": "codium.right_click_enhance_code",
					"group": "b@3"
				},
				{
					"command": "codium.right_click_code_suggestion",
					"group": "b@4"
				}
			],
			"view/title": [
				{
					"command": "codium.auth.accountSignIn",
					"group": "navigation@0",
					"when": "!codium.user.isLoggedIn && view == chat-view"
				},
				{
					"command": "codium.chat.clear",
					"group": "navigation@0",
					"when": "view == chat-view"
				},
				{
					"command": "codium.chat.reload",
					"group": "navigation@0",
					"when": "view == chat-view && codium.user.codiumate"
				}
			]
		},
		"walkthroughs": [
			{
				"id": "codium",
				"title": "CodiumAI",
				"description": "Meaningful tests for busy devs",
				"steps": [
					{
						"id": "sign-in",
						"title": "Sign in",
						"description": "Sign in to get started with CodiumAI.\n[Sign in](command:codium.auth.accountSignIn)",
						"media": {
							"markdown": "walkthrough/sign-in.md"
						},
						"when": "!codium.user.isLoggedIn"
					},
					{
						"id": "generate-tests",
						"title": "Generate Tests",
						"description": "CodiumAI analyzes your code and generates meaningful tests to catch bugs before you ship.",
						"media": {
							"markdown": "walkthrough/generate-tests.md"
						}
					},
					{
						"id": "refine-tests",
						"title": "Refine Suggested Tests",
						"description": "With CodiumAI, you can easily and quickly create comprehensive test suites that help you ensure the reliability and correctness of your software.",
						"media": {
							"markdown": "walkthrough/refine-tests.md"
						}
					},
					{
						"id": "behaviors",
						"title": "Behavior Coverage",
						"description": "Explore your code behaviors and generate tests for each behavior",
						"media": {
							"markdown": "walkthrough/behaviors.md"
						}
					},
					{
						"id": "quick-actions",
						"title": "Quick Actions",
						"description": "Generate tests, get code suggestions and more using Quick Actions panel",
						"media": {
							"markdown": "walkthrough/quick_actions.md"
						}
					},
					{
						"id": "save-tests",
						"title": "Save Tests",
						"description": "Once you are done, you can save the generated tests with a simple click!",
						"media": {
							"markdown": "walkthrough/save-tests.md"
						}
					},
					{
						"id": "code-suggestions",
						"title": "Code Suggestions",
						"description": "CodiumAI helps you to improve your code",
						"media": {
							"markdown": "walkthrough/code-suggestions.md"
						}
					},
					{
						"id": "join-community",
						"title": "Join the community",
						"description": "Whether you need help, have suggestions, or want to share the excitement, join us on Discord!",
						"media": {
							"markdown": "walkthrough/join-community.md"
						}
					}
				]
			}
		]
	},
	"scripts": {
		"dead-code": "knip",
		"build:tests_panel": "cd src/webviews/generate_tests_panel && npm install && npm run build && cd ../../..",
		"build:chat": "cd src/webviews/chat_panel && npm install && npm run build && cd ../../..",
		"build:all": "npm run build:tests_panel && npm run build:chat",
		"prepare": "husky install",
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"clean": "rimraf ./out",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test:contract": "CODIUM_INTEGRATION_TEST_RUN=TRUE node ./out/test/run_test.js",
		"test:unit": "mocha --parallel --require ts-node/register '\"src/test/unit_tests/**/*.test.ts\"'",
		"test": "npm run test:unit && npm run test:contract",
		"vscode:package": "vsce package",
		"vscode:package:e2e": "mkdir -p src/test/suite/test_data/sample_workspace/dist && webpack && vsce package --out src/test/suite/test_data/sample_workspace/dist/codium.e2e.vsix",
		"vscode:publish": "vsce publish",
		"ovsx:publish": "ovsx publish",
		"release": "release-it --dry-run"
	},
